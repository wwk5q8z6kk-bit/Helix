# Copy to .env and fill in values

# LLM Provider API Keys (at least one required)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GOOGLE_API_KEY=
XAI_API_KEY=
DEEPSEEK_API_KEY=

# Additional LLM Providers
OPENROUTER_API_KEY=
MISTRAL_API_KEY=
TOGETHER_API_KEY=
FIREWORKS_API_KEY=
PERPLEXITY_API_KEY=
CLOUDFLARE_API_KEY=
CLOUDFLARE_ACCOUNT_ID=
VENICE_API_KEY=
COHERE_API_KEY=
# AWS Bedrock (uses AWS_ACCESS_KEY_ID, AWS_SECRET_ACCESS_KEY, AWS_DEFAULT_REGION)

# Helix Configuration
HELIX_HOME=~/.helix
HELIX_ENV=development
HELIX_CORS_ORIGINS=http://localhost:3000,http://localhost:8200

# Rust Core
HELIX_RUST_URL=http://127.0.0.1:9470

# Budget Tracking
HELIX_DAILY_BUDGET=100
HELIX_BUDGET_ACTION=downgrade

# Composio Integration
COMPOSIO_API_KEY=
HELIX_TOOL_MODE=sovereign
